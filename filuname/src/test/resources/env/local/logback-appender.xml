<included>
	<appender name="MAIN" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%thread][%-5level][%logger{36}] %msg%n</pattern>
		</encoder>
	</appender>

	<appender name="SQL" class="ch.qos.logback.core.FileAppender">
		<file>target/hibnerate-sql.log</file>
		<encoder>
			<pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%thread][%-5level][%logger{36}] %msg%n</pattern>
		</encoder>
	</appender>
</included>